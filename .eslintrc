{
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb",
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "classes": true,
    "modules": true,
    "jsx": true
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "quotes": [2, "single"],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "comma-dangle": [2, "never"]
  }
}
